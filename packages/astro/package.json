{
  "name": "@tistory-skin-previewer/astro",
  "version": "0.5.0",
  "description": "Astro integration for Tistory skin previewing",
  "author": "Bluenyang",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "lint": "echo 'lint not configured'"
  },
  "dependencies": {
    "@tistory-skin-previewer/core": "*"
  },
  "peerDependencies": {
    "astro": "^5.12.0",
    "vite": "^7.0.5"
  },
  "devDependencies": {
    "astro": "^5.12.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "astro",
    "tistory",
    "skin",
    "preview",
    "integration"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
